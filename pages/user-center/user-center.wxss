.haeder {
  height: 440rpx;
  position: relative;
  padding-top: 68rpx;
  box-sizing: border-box;
  margin-bottom: 17rpx;
}
.haeder_logo{
  width: 200rpx;
  height: 200rpx;
  position: relative;
  margin: 0 auto 12rpx auto;
}
.haeder_logo_bg{
  width: 200rpx;
  height: 200rpx;
}
.haeder_logo_img {
  position: absolute;
  top: 50%; left: 50%;
  transform: translate(-50%,-50%);
  width: 156rpx;
  height: 156rpx;
  border-radius: 100%;
  border: 7rpx solid #fff;
}
.haeder_name{
  display: block;
  color: #fff;
  text-align: center;
  font-size: 28rpx;
}
.haeder_bg{
  width: 100%;
  height: 84rpx;
  position: absolute;
  bottom: 0; left: 0;
}
.Jurisdiction_but{
  width: 229rpx;
  height: 66rpx;
  border-radius: 10rpx;
  line-height: 66rpx;
  color: #333;
  font-size: 28rpx;
  margin-top: 60rpx;
}
.per-main{
  margin:0 auto;
  width: 573rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
}
.per-main-item{
  width: 265rpx;
  height: 183rpx;
  background-color: #f5f6f7;
  margin-top:20rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  border-radius:10rpx;
}
.per-main-item image{
  width: 90rpx;
  height: 90rpx;
  margin-bottom: 25rpx;
}
.per-main-item text{
  font-size: 30rpx;
  color: #acabab;
}